{
  "entrypoint": "index.html",
  "shell": "src/j-site.js",
  "npm": true,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "sources": [
   "src/**",
   "node_modules/j-elements/src/**"
  ],
  "extraDependencies": [
    "manifest.json",
    "_redirects",
    "node_modules/j-elements/docs/**",
    "node_modules/j-elements/src/**"
  ],
  "builds": [
    {
      "name": "unbundled",
      "bundle": false,
      "js": {"minify": true, "compile": "es2015"},
      "css": {"minify": true},
      "html": {"minify": true},
      "addServiceWorker": true,
      "addPushManifest": true
    }
  ]
}
